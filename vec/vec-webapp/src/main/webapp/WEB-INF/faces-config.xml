<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		<locale-config>
			<default-locale>ru_RU</default-locale>
			<supported-locale>ru_RU</supported-locale>
			<supported-locale>en_EN</supported-locale>
		</locale-config>
	</application>
	<application>
		<resource-bundle>
			<base-name>strings</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
	<converter>
		<converter-id>course</converter-id>
		<converter-class>
			ru.sstu.vec.core.web.converters.CourseConverter
		</converter-class>
	</converter>
	<navigation-rule>
		<navigation-case>
			<from-outcome>usersList</from-outcome>
			<to-view-id>/pages/usersList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>userInfo</from-outcome>
			<to-view-id>/pages/userInfo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>groupsList</from-outcome>
			<to-view-id>/pages/groupsList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>groupInfo</from-outcome>
			<to-view-id>/pages/groupInfo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>expertCoursesList</from-outcome>
			<to-view-id>/pages/expertCoursesList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>expertCourseInfo</from-outcome>
			<to-view-id>/pages/expertCourseInfo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>expertLabInfo</from-outcome>
			<to-view-id>/pages/expertLabInfo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>expertVariantInfo</from-outcome>
			<to-view-id>/pages/expertVariantInfo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>studentCoursesList</from-outcome>
			<to-view-id>/pages/studentCoursesList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>studentCourseInfo</from-outcome>
			<to-view-id>/pages/studentCourseInfo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>studentLabInfo</from-outcome>
			<to-view-id>/pages/studentLabInfo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
