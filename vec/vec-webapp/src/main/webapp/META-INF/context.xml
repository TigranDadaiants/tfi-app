<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Context>
<Context path="/vec">
	<Resource
		auth="Container"
		name="jdbc/vec"
		type="javax.sql.DataSource"
		password="admin"
		driverClassName="org.h2.Driver"
		maxIdle="2"
		maxWait="5000"
		username="admin"
		url="jdbc:h2:~/vec"
		maxActive="4"
		characterEncoding="UTF-8"/>
</Context>
